<domain type='kvm'>
  <name>provisioner-0</name>
  <memory unit='GiB'>4</memory>
  <currentMemory unit='GiB'>4</currentMemory>
  <vcpu placement='static'>2</vcpu>
  <os>
    <type arch='x86_64' machine='pc'>hvm</type>
    <kernel>/data/pv/matchbox/assets/coreos/1883.1.0/coreos_production_pxe.vmlinuz</kernel>
    <initrd>/data/pv/matchbox/assets/coreos/1883.1.0/coreos_production_pxe_image.cpio.gz</initrd>
    <cmdline>coreos.first_boot=1 coreos.autologin coreos.config.url=http://r.fuzzybunny.io/ignition/provisioner-0.ign console=hvc0 elevator=noop</cmdline>
    <boot dev='hd'/>
  </os>
  <features>
    <acpi/>
    <apic/>
    <pae/>
  </features>
  <cpu mode='host-passthrough'>
    <topology sockets='1' cores='2' threads='1'/>
  </cpu>
  <clock offset='utc'/>
  <on_poweroff>destroy</on_poweroff>
  <on_reboot>restart</on_reboot>
  <on_crash>restart</on_crash>
  <devices>
    <emulator>/usr/bin/qemu-system-x86_64</emulator>
    <interface type='direct' trustGuestRxFilters='yes'>
      <source dev='enp1s0f1' mode='bridge'/>
      <model type='virtio-net'/>
    </interface>
    <interface type='direct' trustGuestRxFilters='yes'>
      <source dev='enp1s0f0' mode='bridge'/>
      <model type='virtio-net'/>
    </interface>
    <interface type='direct' trustGuestRxFilters='yes'>
      <mac address='52:54:00:63:6e:b2'/>
      <source dev='eno2' mode='bridge'/>
      <model type='virtio-net'/>
    </interface>
    <console>
      <target type='virtio'/>
    </console>
    <channel type='spicevmc'>
      <target type='virtio' name='com.redhat.spice.0'/>
      <address type='virtio-serial'/>
    </channel>
    <memballoon model='virtio'>
    </memballoon>
  </devices>
</domain>
