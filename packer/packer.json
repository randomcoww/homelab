{
  "builders": [
    {
      "boot_command" : [
        "<tab> ",
        "inst.ks=http://{{.HTTPIP}}:8080/generic?host={{user `kickstart`}} ",
        "<enter>"
      ],
      "disk_size" : 30000,
      "headless" : false,
      "http_directory" : "packer",
      "iso_checksum_type": "none",
      "iso_url": "https://download.fedoraproject.org/pub/fedora/linux/releases/28/Server/x86_64/iso/Fedora-Server-netinst-x86_64-28-1.1.iso",
      "shutdown_command" : "",
      "disk_interface": "virtio",
      "net_device": "virtio-net",
      "communicator": "none",
      "type": "qemu",
      "shutdown_timeout": "30m",
      "qemuargs": [
        [ "-m", "4096M" ],
        [ "-smp", "2" ]
      ],
      "accelerator": "kvm",
      "vm_name": "{{user `kickstart`}}",
      "format": "qcow2",
      "output_directory": "output-qemu"
    }
  ],
  "variables": {
    "kickstart": "store-0"
  }
}
