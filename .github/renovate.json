{
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^config_env.tf$",
        "^README.md$"
      ],
      "matchStrings": [
        "(?<depName>gcr.io\/.+\/.+?):(?<currentValue>[\\w.-]+)",
        "(?<depName>ghcr.io\/.+\/.+?):(?<currentValue>[\\w.-]+)",
        "(?<depName>docker.io\/.+\/.+?):(?<currentValue>[\\w.-]+)",
        "(?<depName>quay.io\/.+\/.+?):(?<currentValue>[\\w.-]+)",
        "(?<depName>registry.k8s.io\/.+\/.+?):(?<currentValue>[\\w.-]+)"
      ],
      "datasourceTemplate": "docker"
    },
    {
      "customType": "regex",
      "fileMatch": [
        "\\.tf$"
      ],
      "matchStrings": [
        "^\\s*helm_template\\s*=\\s*{[^}]+\\s+repository\\s*=\\s*\"(?<registryUrl>.+)\"\\s+chart\\s*=\\s*\"(?<depName>[\\w-]+)\"\\s+version\\s*=\\s*\"(?<currentValue>[\\d.]+)\""
      ],
      "datasourceTemplate": "helm"
    }
  ]
}