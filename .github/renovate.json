{
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^config_env.tf$",
        "^README.md$"
      ],
      "matchStrings": [
        "(?<depName>[a-z0-9.-]+(:\\d+|)\/[a-z0-9-]+(?:\/[a-z0-9-]+|)):(?<currentValue>[\\w.-]{1,127})(@(?<currentDigest>sha256:[a-f0-9]+))?"
      ],
      "datasourceTemplate": "docker",
      "versioningTemplate": "docker"
    },
    {
      "customType": "regex",
      "fileMatch": [
        "^config_env.tf$"
      ],
      "matchStrings": [
        "pxeboot_images\\s*=\\s*{[^}]*\\s+\"[\\w-]+-(?<currentValue>[\\d.]+)\"\\s+#\\s+(?<depName>[\\w\/-]+)"
      ],
      "datasourceTemplate": "github-tags"
    }
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "github-tags"
      ],
      "matchPackageNames": [
        "randomcoww/fedora-coreos-config"
      ],
      "versioning": "docker"
    },
    {
      "matchDatasources": [
        "docker"
      ],
      "pinDigests": true
    },
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true,
      "ignoreTests": true
    }
  ]
}