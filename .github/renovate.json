{
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^config_env.tf$",
        "^README.md$"
      ],
      "matchStrings": [
        "(?<depName>[a-z0-9.-_]+(?:\/[a-z0-9.-_]+|)):(?<currentValue>[\\w.-_]{1,127})"
      ],
      "datasourceTemplate": "docker"
    },
    {
      "customType": "regex",
      "fileMatch": [
        "\\.tf$"
      ],
      "matchStrings": [
        "^data\\s+\"helm_template\"\\s*\"[\\w_-]+\"\\s*(?={[^}]*\\s+repository\\s*=\\s*\"(?<registryUrl>.+)\")(?=[^}]*\\s+version\\s*=\\s*\"(?<currentValue>[\\d.]+)\")(?=[^}]*\\s+chart\\s*=\\s*\"(?<depName>[\\w-]+)\")"
      ],
      "datasourceTemplate": "helm"
    },
    {
      "customType": "regex",
      "fileMatch": [
        "^config_env.tf$"
      ],
      "matchStrings": [
        "pxeboot_images\\s*=\\s*{[^}]*\\s+\"[\\w-]+-(?<currentValue>[\\d.]+)\"\\s+#\\s+(?<depName>[\\w\/-]+)"
      ],
      "datasourceTemplate": "github-tags"
    }
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "docker.io/searxng/searxng"
      ],
      "versioning": "regex:^(?<major>\\d+)?(.(?<minor>\\d+))?(.(?<patch>\\d+))?(-.*)?$",
      "automerge": true,
      "ignoreTests": true
    },
    {
      "matchDatasources": [
        "github-tags"
      ],
      "matchPackageNames": [
        "randomcoww/fedora-coreos-config"
      ],
      "versioning": "docker"
    },
    {
      "matchUpdateTypes": ["minor", "patch"],
      "automerge": true,
      "ignoreTests": true
    }
  ]
}