{
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "^config_env.tf$",
        "^README.md$",
        "^renovate.yaml$"
      ],
      "matchStrings": [
        "(?<depName>[a-z0-9.-]+(:\\d+|)\/[a-z0-9-]+(?:\/[a-z0-9-]+|)):(?<currentValue>[\\w.-]{1,127})"
      ],
      "datasourceTemplate": "docker"
    },
    {
      "customType": "regex",
      "fileMatch": [
        "^config_env.tf$"
      ],
      "matchStrings": [
        "pxeboot_images\\s*=\\s*{[^}]*\\s+\"[\\w-]+-(?<currentValue>[\\d.]+)\"\\s+#\\s+(?<depName>[\\w\/-]+)"
      ],
      "datasourceTemplate": "github-tags"
    }
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "docker.io/searxng/searxng"
      ],
      "versioning": "regex:^(?<major>\\d+)?(.(?<minor>\\d+))?(.(?<patch>\\d+))?(-.*)?$",
      "automerge": true,
      "ignoreTests": true
    },
    {
      "matchDatasources": [
        "github-tags"
      ],
      "matchPackageNames": [
        "randomcoww/fedora-coreos-config"
      ],
      "versioning": "docker"
    },
    {
      "matchUpdateTypes": ["minor", "patch"],
      "automerge": true,
      "ignoreTests": true
    }
  ]
}